{
  "manifest_version": 2,
  "name": "Wallet",
  "description": "Wallet extension",
  "version": "1.0.0",
  "icons": {
    "16": "wallet_icon.png",
    "48": "wallet_icon.png",
    "128": "wallet_icon.png"
  },
  "browser_action": {
    "default_icon": "wallet_icon.png",
    "default_popup": "popup.html"
  },
  "permissions": ["activeTab", "tabs", "storage"],
  "background": {
    "scripts": ["js/background.js"],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["js/script.js", "js/jquery-2.0.3.min.js"],
      "run_at": "document_end"
    }
  ]
}